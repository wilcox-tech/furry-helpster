<h1>Listing versions</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Name</th>
    <th>State</th>
    <th>Private</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @versions.each do |version| %>
  <tr>
    <td><%= version.product.name %></td>
    <td><%= version.name %></td>
    <td><%= version.state %></td>
    <td><%= version.private %></td>
    <td><%= link_to 'Show', version %></td>
    <td><%= link_to 'Edit', edit_admin_version_path(version) %></td>
    <td><%= link_to 'Destroy', version, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Version', new_admin_version_path %>
