<div class="navbar">
  <div class="navbar-inner">
    <%= link_to t('.app_title'), root_path, :class => 'brand' %>
    <ul class="nav">
      <li><%= link_to t('.videos'), '#' %></li>
      <li><%= link_to t('.doc_hub'), '#' %></li>
      <li><%= link_to t('.community'), '#' %></li>
    </ul>
    <%= form_tag({}, {:class => 'navbar-search pull-right'}) do %>
      <%= text_field_tag :search, params[:search], :class => 'search-query', :placeholder => t('.search_placeholder') %>
    <% end %>
  </div>
</div>