<h1>Listing doc_groups</h1>

<table>
  <tr>
    <th>Version</th>
    <th>Name</th>
    <th>Private</th>
    <th>Primary doc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @doc_groups.each do |doc_group| %>
  <tr>
    <td><%= doc_group.version_id %></td>
    <td><%= doc_group.name %></td>
    <td><%= doc_group.private %></td>
    <td><%= doc_group.primary_doc_id %></td>
    <td><%= link_to 'Show', doc_group %></td>
    <td><%= link_to 'Edit', edit_admin_doc_group_path(doc_group) %></td>
    <td><%= link_to 'Destroy', doc_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Doc group', new_admin_doc_group_path %>
